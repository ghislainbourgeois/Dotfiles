set editing-mode vi

set show-mode-in-prompt on
set vi-cmd-mode-string "\1[31m\2C \1(B[m\2\1[33m\2"
set vi-ins-mode-string "\1[32m\2I \1(B[m\2\1[33m\2"

set show-all-if-ambiguous on

set completion-ignore-case on

set menu-complete-display-prefix on

set keymap vi-command

set keymap vi-insert

TAB: menu-complete

# For some reason, p seems to break in insert mode
"p": self-insert
